<div class="card">
  {{#if thumbnail.length}}
    {{#each thumbnail}}
      <img src={{this}} alt={{title}}>
    {{/each}}
  {{else}}
    <img src="https://st4.depositphotos.com/14953852/24651/v/450/depositphotos_246517344-stock-illustration-image-available-icon-vector-flat.jpg" alt="o image">
  {{/if}}
  <h3>{{title}}</h3>
  <p>Category: {{category}} </p>
  <p>Price: ${{price}}</p>
  <p>Description: {{description}} </p>
</div>